# AWS Configuration
AWS_REGION=ap-south-1
AWS_ACCOUNT_ID=your-account-id
ENVIRONMENT=dev

# Cognito Configuration (populated after deployment)
COGNITO_USER_POOL_ID=
COGNITO_CLIENT_ID=
COGNITO_DOMAIN=

# API Configuration (populated after deployment)
API_URL=

# Bedrock Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0
BEDROCK_HAIKU_MODEL_ID=anthropic.claude-3-haiku-20240307-v1:0

# Database Configuration (managed by CDK)
DB_NAME=samaansathi

# S3 Buckets (populated after deployment)
DATA_BUCKET=
BILLS_BUCKET=
MODELS_BUCKET=

# DynamoDB Tables (populated after deployment)
INVENTORY_TABLE=samaan-sathi-inventory
SESSIONS_TABLE=samaan-sathi-sessions
UDHAAR_TABLE=samaan-sathi-udhaar

# Monitoring
LOG_LEVEL=INFO
ENABLE_XRAY=true

# Feature Flags
ENABLE_ML_FORECASTING=true
ENABLE_BEDROCK_RECOMMENDATIONS=true
ENABLE_OCR_PROCESSING=true
